{
    "name": "spiewajacestudio.pl",
    "version": "1.0.0",
    "description": "Website for Śpiewające Studio",
    "main": "index.js",
    "config": {
        "liveFolder": "live",
        "buildFolder": "docs",
        "deployWorkingFolder": "deploy",
        "deployOutputFolder": "deploy/DEPLOY_OUTPUT"
    },
    "scripts": {
        "start": "npm run live:pre && npm run live:sass && npm run live:sass:watch | npm run live:html:watch | live-server $npm_package_config_liveFolder/",
        "build": "npm run build:pre && npm run build:html && npm run build:css",
        "deploy": "npm run deploy:pre && npm run deploy:html && npm run deploy:css && npm run deploy:assets && npm run deploy:favicon && npm run deploy:copy:only_deploy_other",
        "live:pre": "npm run live:pre:version && npm run live:pre:mkdirs && npm run live:pre:copy-assets && npm run live:pre:copy-favicon",
        "live:pre:version": "echo Development session started $(date +\"%Y-%m-%d_%H:%M.%3N\") > src/_auto-version.html",
        "live:pre:mkdirs": "mkdir -p $npm_package_config_liveFolder/css && mkdir -p $npm_package_config_liveFolder/img && mkdir -p $npm_package_config_liveFolder/favicon",
        "live:pre:copy-assets": "cp -r src/img/* $npm_package_config_liveFolder/img",
        "live:pre:copy-favicon": "cp -r src/favicon/* $npm_package_config_liveFolder/favicon",
        "live:sass": "node-sass src/sass/main.scss $npm_package_config_liveFolder/css/style.css",
        "live:sass:watch": "node-sass src/sass/main.scss $npm_package_config_liveFolder/css/style.css -w",
        "live:html:watch": "html-includes --src src --dest $npm_package_config_liveFolder --watch",
        "live:prefix": "postcss --use autoprefixer -b 'last 10 versions' $npm_package_config_liveFolder/css/style.css -o $npm_package_config_liveFolder/css/style.css",
        "build:pre": "npm run build:pre:version && npm run build:pre:clean && npm run build:pre:mkdirs && npm run build:pre:copy-assets && npm run build:pre:copy-favicon",
        "build:pre:version": "echo Build for testing/preview $(date +\"%Y-%m-%d_%H:%M.%3N\") > src/_auto-version.html",
        "build:pre:clean": "rimraf $npm_package_config_buildFolder",
        "build:pre:mkdirs": "mkdir -p $npm_package_config_buildFolder/css && mkdir -p $npm_package_config_buildFolder/img && mkdir -p $npm_package_config_buildFolder/favicon",
        "build:pre:copy-assets": "cp -r src/img/* $npm_package_config_buildFolder/img",
        "build:pre:copy-favicon": "cp -r src/favicon/* $npm_package_config_buildFolder/favicon",
        "build:css": "npm-run-all build:css:compile-sass build:css:concat-css build:css:autoprefix build:css:compress-css",
        "build:css:compile-sass": "node-sass src/sass/main.scss src/css/style.comp.css",
        "build:css:concat-css": "concat -o src/css/style.concat.css src/css/style.comp.css",
        "build:css:autoprefix": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.concat.css -o src/css/style.prefix.css",
        "build:css:compress-css": "node-sass src/css/style.prefix.css $npm_package_config_buildFolder/css/style.css --output-style compressed",
        "build:html": "html-includes --src src --dest $npm_package_config_buildFolder",
        "deploy:pre": "npm run deploy:pre:cleanOutpur",
        "deploy:pre:cleanOutpur": "rimraf $npm_package_config_deployOutputFolder",
        "deploy:html": "npm run deploy:html:mkdirs-temp && npm run deploy:html:copy-html && npm run deploy:html:version-update && npm run deploy:html:copy-only-deploy-html && npm run deploy:html:compile && npm run deploy:html:clean-temp",
        "deploy:html:mkdirs-temp": "mkdir -p $npm_package_config_deployWorkingFolder/deployTemp",
        "deploy:html:copy-html": "find src -maxdepth 1 -name '*.html' -type f -exec cp {} $npm_package_config_deployWorkingFolder/deployTemp \\;",
        "deploy:html:version-update": "echo DEPLOY $(date +\"%Y-%m-%d_%H:%M.%3N\") > $npm_package_config_deployWorkingFolder/deployTemp/_auto-version.html",
        "deploy:html:copy-only-deploy-html": "cp -r $npm_package_config_deployWorkingFolder/deploy_helpers/only_deploy_html_partials $npm_package_config_deployWorkingFolder/deployTemp",
        "deploy:html:compile": "html-includes --src $npm_package_config_deployWorkingFolder/deployTemp --dest $npm_package_config_deployOutputFolder",
        "deploy:html:clean-temp": "rimraf $npm_package_config_deployWorkingFolder/deployTemp",
        "deploy:css": "npm run deploy:css:mkdirs && npm run deploy:css:compile-sass && npm run deploy:css:concat-css && npm run deploy:css:autoprefix && npm run deploy:css:compress-css",
        "deploy:css:mkdirs": "mkdir -p $npm_package_config_deployOutputFolder/css",
        "deploy:css:compile-sass": "node-sass src/sass/main.scss src/css/style.comp.css",
        "deploy:css:concat-css": "concat -o src/css/style.concat.css src/css/style.comp.css",
        "deploy:css:autoprefix": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.concat.css -o src/css/style.prefix.css",
        "deploy:css:compress-css": "node-sass src/css/style.prefix.css $npm_package_config_deployOutputFolder/css/style.css --output-style compressed",
        "deploy:assets": "npm run deploy:assets:mkdirs && npm run deploy:assets:copy",
        "deploy:assets:mkdirs": "mkdir -p $npm_package_config_deployOutputFolder/img",
        "deploy:assets:copy": "cp -r src/img/* $npm_package_config_deployOutputFolder/img",
        "deploy:favicon": "npm run deploy:favicon:mkdirs && npm run deploy:favicon:copy",
        "deploy:favicon:mkdirs": "mkdir -p $npm_package_config_deployOutputFolder/favicon",
        "deploy:favicon:copy": "cp -r src/favicon/* $npm_package_config_deployOutputFolder/favicon",
        "deploy:copy:only_deploy_other": "cp -r $npm_package_config_deployWorkingFolder/deploy_helpers/only_deploy_other/. $npm_package_config_deployOutputFolder"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jurek2006/spiewajacestudio.pl.git"
    },
    "author": "Jurek Skowron",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jurek2006/spiewajacestudio.pl/issues"
    },
    "homepage": "https://github.com/jurek2006/spiewajacestudio.pl#readme",
    "devDependencies": {
        "autoprefixer": "^9.6.1",
        "concat": "^1.0.3",
        "html-includes": "^4.2.0",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^6.1.3"
    },
    "dependencies": {}
}
